<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>River.Streaming</PackageId>
    <Version>0.9.0</Version>
    <Authors>John Susi</Authors>
    <Summary>River.Streaming is a library for building concurrent applications in C#</Summary>
    <RepositoryUrl>https://github.com/johnsusi/river-dotnet/</RepositoryUrl>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>8</LangVersion>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Threading.Channels" Version="4.7.1" />
  </ItemGroup>

</Project>
